{
  "name": "swtc-factory",
  "version": "1.2.5",
  "description": "swtc base lib factory",
  "main": "src/index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "prettier": "prettier",
    "pretty-quick": "pretty-quick",
    "prettier:check": "prettier -c tssrc/*.ts",
    "prettier:tabs": "prettier --use-tabs --write package.json tssrc/*.ts",
    "tsc": "tsc",
    "tslint": "tslint tssrc/*.ts",
    "test": "mocha \"test/test*.js\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/swtcca/swtc-factory.git"
  },
  "keywords": [
    "factory",
    "swtc",
    "jcc"
  ],
  "author": "Lospring Liu <lospringliu@gmail.com>",
  "license": "BSD-2-Clause",
  "bugs": {
    "url": "https://github.com/swtcca/swtc-factory/issues"
  },
  "homepage": "https://github.com/swtcca/swtc-factory#readme",
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged --no-restage --verbose --bail"
    }
  },
  "dependencies": {
    "swtc-keypairs": "^0.11.15"
  },
  "devDependencies": {
    "@types/node": "^11.12.2",
    "chai": "^4.2.0",
    "eslint": "^5.15.3",
    "esm": "^3.2.6",
    "husky": "1.3.1",
    "mocha": "^5.2.0",
    "nyc": "^14.0.0",
    "prettier": "^1.16.4",
    "pretty-quick": "^1.10.0",
    "tslint": "^5.15.0",
    "typescript": "^3.4.1"
